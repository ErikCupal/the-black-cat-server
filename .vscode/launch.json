{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "--harmony"
      ],
      "protocol": "inspector",
      "program": "${workspaceRoot}/src/index.ts",
      "cwd": "${workspaceRoot}",
      "outFiles": [
        "${workspaceRoot}/build/**/*.js"
      ],
      "sourceMaps": true
    }
  ]
}