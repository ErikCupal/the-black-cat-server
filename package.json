{
  "name": "the-black-cat-server",
  "version": "0.1.0",
  "description": "The server part of online multiplayer card game The Black Cat",
  "main": "./build/index.js",
  "scripts": {
    "build": "webpack",
    "start": "npm run build && node --harmony ./build/index.js",
    "launch": "node --harmony ./build/index.js",
    "test": "jest",
    "lint": "tslint ./src/**/*.ts ./tests/**/*.ts",
    "doc": "typedoc --options typedoc.json"
  },
  "author": {
    "name": "Erik Cupal",
    "email": "erik@cup.al"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ErikCupal/the-black-cat-server"
  },
  "license": "MIT",
  "dependencies": {
    "express": "^4.14.0",
    "redux": "^3.6.0",
    "returnof": "^1.0.0",
    "rxjs": "^5.1.1",
    "socket.io": "^1.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/jest": "^19.2.2",
    "@types/remote-redux-devtools": "^0.3.5",
    "@types/socket.io": "^1.4.28",
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "jest": "^19.0.2",
    "remote-redux-devtools": "^0.5.7",
    "ts-jest": "^19.0.1",
    "ts-loader": "^2.0.0",
    "tslint": "^4.5.1",
    "typedoc": "^0.5.7",
    "typescript": "^2.3.0-dev.20170327",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".(ts|tsx)$": "<rootDir>/jest.preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "constants"
    ]
  }
}
